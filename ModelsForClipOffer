import lombok.Builder;
import lombok.Getter;
import lombok.Setter;

@Getter
@Setter
@Builder
public class ClipJ4UResponse {
	private String ack;

}


import lombok.Getter;
import lombok.Setter;

@Getter
@Setter
public class ClippOfferRequest {

	List<Offer> offers;

}



@Getter
@Setter
@Builder
public class Item {

	private String clipType;
	private String itemId;
	private String itemType;

}


@Getter
@Setter
@Builder
public class Offer {

	private String offerId;
	private String offerPgm;

}


